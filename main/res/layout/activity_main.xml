<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main_Activity.MainActivity">


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="411dp"
        android:layout_height="739dp"
        android:scaleType="fitXY"
        android:src="@drawable/fond"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />


    <ImageButton
        android:id="@+id/btn_create"
        style="@android:style/Widget.Material.ImageButton"
        android:layout_width="195dp"
        android:layout_height="125dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/fond"
        android:scaleType="fitXY"
        android:src="@drawable/flech_deg_h"
        app:layout_constraintBottom_toTopOf="@+id/btn_settings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:targetApi="lollipop"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/btn_settings"
        style="@android:style/Widget.Material.ImageButton"
        android:layout_width="195dp"
        android:layout_height="125dp"
        android:layout_marginTop="348dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/fond"
        android:scaleType="fitXY"
        android:src="@drawable/flech_deg_d"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:targetApi="lollipop"
        tools:ignore="ContentDescription" />

    <ImageButton
        android:id="@+id/btn_log"
        style="@android:style/Widget.Material.ImageButton"
        android:layout_width="195dp"
        android:layout_height="125dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:background="@drawable/fond"
        android:scaleType="fitXY"
        android:src="@drawable/flech_deg_b"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_settings"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:targetApi="lollipop" />


    <ImageButton
        android:id="@+id/btn_join"
        style="@android:style/Widget.Material.ImageButton"
        android:layout_width="195dp"
        android:layout_height="125dp"
        android:layout_marginTop="348dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/fond"
        android:scaleType="fitXY"
        android:src="@drawable/flech_deg_g"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="ContentDescription,MissingConstraints"
        tools:targetApi="lollipop" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="58dp"
        android:layout_height="25dp"
        android:text="@string/setting"
        android:textColor="@color/colorBackground"
        app:layout_constraintBottom_toBottomOf="@+id/btn_create"
        app:layout_constraintEnd_toEndOf="@+id/btn_create"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="@+id/btn_create"
        app:layout_constraintTop_toTopOf="@+id/btn_create"
        app:layout_constraintVertical_bias="0.29" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="71dp"
        android:layout_height="22dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:text="@string/join_party"
        android:textColor="@color/colorBackground"
        app:layout_constraintBottom_toBottomOf="@+id/btn_join"
        app:layout_constraintEnd_toEndOf="@+id/btn_join"
        app:layout_constraintHorizontal_bias="0.296"
        app:layout_constraintStart_toStartOf="@+id/btn_join"
        app:layout_constraintTop_toTopOf="@+id/btn_join"
        app:layout_constraintVertical_bias="0.485" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="90dp"
        android:layout_height="23dp"
        android:text="@string/create_party"
        android:textColor="@color/colorBackground"
        app:layout_constraintBottom_toBottomOf="@+id/btn_settings"
        app:layout_constraintEnd_toEndOf="@+id/btn_settings"
        app:layout_constraintHorizontal_bias="0.779"
        app:layout_constraintStart_toStartOf="@+id/btn_settings"
        app:layout_constraintTop_toTopOf="@+id/btn_settings" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="59dp"
        android:layout_height="23dp"
        android:text="@string/register_or_login"
        android:textColor="@color/colorBackground"
        app:layout_constraintBottom_toBottomOf="@+id/btn_log"
        app:layout_constraintEnd_toEndOf="@+id/btn_log"
        app:layout_constraintHorizontal_bias="0.573"
        app:layout_constraintStart_toStartOf="@+id/btn_log"
        app:layout_constraintTop_toTopOf="@+id/btn_log"
        app:layout_constraintVertical_bias="0.696" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="286dp"
        android:layout_height="109dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="48dp"
        android:scaleType="fitXY"
        android:src="@drawable/sweepnplay2"
        app:layout_constraintBottom_toTopOf="@+id/btn_create"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/sweepnplay2"
        tools:ignore="ContentDescription,MissingConstraints" />


</android.support.constraint.ConstraintLayout>